{
  "name": "@loveholidays/launchpad-cli",
  "version": "1.0.0",
  "description": "LoveHolidays Launchpad CLI tool",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "launchpad": "dist/bin/launchpad.js"
  },
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev": "tsx --watch src/bin/launchpad.ts",
    "lint": "biome lint . && oxlint .",
    "lint:fix": "biome lint --write . && biome format --write .",
    "format": "biome format .",
    "format:write": "biome format --write .",
    "check": "biome check .",
    "check:write": "biome check --write .",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "start": "tsx src/bin/launchpad.ts",
    "build:watch": "tsup --config tsup.config.ts --watch",
    "dev:build": "tsup --config tsup.config.ts --watch",
    "start:built": "node dist/bin/launchpad.js"
  },
  "dependencies": {
    "chalk": "5.4.1",
    "commander": "14.0.0",
    "execa": "9.5.3",
    "fs-extra": "11.3.0",
    "inquirer": "12.6.2",
    "parse-json": "8.3.0",
    "safe-stable-stringify": "2.5.0",
    "trash": "9.0.0",
    "ts-pattern": "5.7.1",
    "zx": "8.5.4"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.4",
    "@types/inquirer": "9.0.8",
    "@types/node": "22.15.21",
    "@types/trash": "5.0.1",
    "tsup": "8.5.0",
    "tsx": "4.19.4",
    "vitest": "2.1.8"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
